{
    "name": "bdmtra/craft-feed-me-link-meta-tags",
    "description": "Append meta tags to feed by url in it",
    "type": "craft-plugin",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.7.0"
    },
    "autoload": {
        "psr-4": {
            "bdmtra\\craftfeedmelinkmetatags\\": "src/"
        }
    },
    "extra": {
        "handle": "_feed-me-link-meta-tags",
        "name": "Feed Me Link Meta Tags",
        "developer": "Dmitry Borovkov",
        "documentationUrl": "",
        "class": "bdmtra\\craftfeedmelinkmetatags\\FeedMeMetaTagParser"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

